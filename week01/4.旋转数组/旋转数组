class Solution {
    public void rotate(int[] nums, int k) {
        for(int i = 0; i < k ; i++){
            int tail = nums[nums.length-1];
            int temp;
            for(int j = 0; j < nums.length; j++){
                temp = nums[j];
                nums[j] = tail;
                tail = temp;
            }
        }
    }
}